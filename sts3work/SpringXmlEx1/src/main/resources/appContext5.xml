<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
	
	<bean id="info" class="spring.di.ex5.SawonInfo">
		<constructor-arg value="김철수"/>
		<constructor-arg value="부장"/>
		<property name="pay" value="4000000"/>
		<property name="buseo">
			<list>
				<value>재경팀</value>
				<value>공사관리팀</value>
				<value>개발팀</value>
			</list>
		</property>
	</bean>
	
	<bean id="emp" class="spring.di.ex5.Emp">
		<constructor-arg value="쌍용"/>
		<property name="info" ref="info"/>
		<property name="emploc" value="서울시 중랑구"/>
	</bean>

</beans>

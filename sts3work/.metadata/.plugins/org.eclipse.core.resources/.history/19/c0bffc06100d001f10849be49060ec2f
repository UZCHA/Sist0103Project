<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="spring.db.carmember.CarMemberDao">
 
 <select id="getAllOfMember" resultType="cmdto">
 	select * from carmember order by num desc
 </select>
 
 
 <select id="TotalCountOfMember" resultType="int">
 	select count(*) from carmember 
 </select>
 
 
 <insert id="insertOfMember" parameterType="cmdto">
 	insert into carmember(name,hp,addr,gaipday) values(#{name},#{hp},#{addr},#{gaipday})
 </insert>
 
 
 <select id="selectOfOneMember" parameterType="String" resultType="cmdto">
 	select from carmember where num=#{num}
 </select>
 
 
 <update id="updateOfMember" parameterType="cmdto">
 	update carmember set name=#{name},hp=#{hp},addr=#{addr},gaipday=#{gaipday} where num=#{num}
 </update>
 </mapper>